<?xml version="1.0"?>
<component name="org.nuxeo.platform.birt.reporting.doctypeContrib">

  <extension target="org.nuxeo.ecm.core.schema.TypeService"
    point="doctype">
    <doctype name="BirtReportModel" extends="File">
      <schema name="birtreportmodel" />
      <facet name="Commentable" />
      <facet name="Versionable" />
    </doctype>
    <doctype name="BirtReport" extends="Document">
      <schema name="common" />
      <schema name="dublincore" />
      <schema name="files" />
      <schema name="birtreport" />
      <facet name="Commentable" />
      <facet name="Versionable" />
    </doctype>
    <doctype name="BirtReportModelsRoot" extends="Folder">
      <facet name="SuperSpace"/>
      <facet name="HiddenInNavigation"/>
    </doctype>
  </extension>

</component>
