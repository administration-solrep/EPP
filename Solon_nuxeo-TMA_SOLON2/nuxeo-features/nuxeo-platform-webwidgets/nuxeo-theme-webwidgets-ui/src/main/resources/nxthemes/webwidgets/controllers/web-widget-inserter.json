  {"id": "web widget inserter",
   "type": "drag-and-drop",
   "dragging": {
       "element": "nxthemesWebWidgetFactory",
       "offset_x": -5,
       "offset_y": -5,
       "feedback": {
           "opacity": 0.8,
           "clone": false,
           "background": "#fce94f",
           "border": "#000"
        },
        "zoomback": {
            "duration": 600
        },
        "opacity": 1
    },
    "dropping": {
        "target": "nxthemesWebWidgetContainer",
        "action": "insert web widget"
     }
  }
