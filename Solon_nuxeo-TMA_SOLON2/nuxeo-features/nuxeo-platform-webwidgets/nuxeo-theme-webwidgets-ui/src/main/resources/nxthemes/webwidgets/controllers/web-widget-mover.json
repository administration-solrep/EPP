  {"id": "web widget mover",
   "type": "drag-and-drop",
   "dragging": {
     "element": "nxthemesWebWidget",
     "handle": "nxthemesWebWidgetDragHandle",
     "feedback": {
       "opacity": 1,
       "clone": true
     },
     "source": {
       "color": "#ccc"
     }
   },
   "shifting": {
     "element": "nxthemesWebWidget",
     "container": "nxthemesWebWidgetContainer"
   },
   "dropping": {
     "action": "move web widget",
     "zoomto": {
       "duration": 200
     }
   }
 }
