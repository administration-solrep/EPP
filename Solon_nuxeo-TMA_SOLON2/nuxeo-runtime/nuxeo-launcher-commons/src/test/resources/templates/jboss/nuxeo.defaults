## DO NOT EDIT THIS FILE, USE nuxeo.conf  ##

# Default configuration values for templates
# Specific default values are defined in templates/{template_name}/nuxeo.defaults

# Default template to use if not defined in nuxeo.conf
nuxeo.templates=default

# Default IP address binding
nuxeo.bind.address=0.0.0.0

# Various other default values
nuxeo.db.host=
nuxeo.db.port=
nuxeo.db.name=
nuxeo.db.user=
nuxeo.db.password=
nuxeo.db.min-pool-size=
nuxeo.db.max-pool-size=20

nuxeo.config.jboss=server/default/deploy/nuxeo.ear/config
nuxeo.datasources.jboss=server/default/deploy/nuxeo.ear/datasources
nuxeo.config.tomcat=nxserver/config
nuxeo.datasources.tomcat=conf/Catalina/localhost/nuxeo.xml

test.nuxeo.defaults = true
test.nuxeo.defaults.template.1 = nuxeo.defaults
test.nuxeo.conf.override.defaults = nuxeo.defaults
