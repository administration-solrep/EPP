<?xml version="1.0"?>

<component name="org.nuxeo.platform.spreadsheet.web.ui.resources.contrib">

  <require>org.nuxeo.web.ui.resources</require>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="resources">
    <resource name="nuxeo-spreadsheet.html" type="import" shrinkable="false">
      <uri>/ui/nuxeo-spreadsheet/nuxeo-spreadsheet.html</uri>
    </resource>
  </extension>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="bundles">
    <bundle name="web-ui">
      <resources append="true">
        <resource>nuxeo-spreadsheet.html</resource>
      </resources>
    </bundle>
  </extension>

</component>