<!--[if gt IE 8]><!-->  <html class="no-js" lang="en"> <!--<![endif]-->
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  <title>${doc.dublincore.title}</title>

  <meta name="description" content="${doc.dublincore.description}">
  <meta name="author" content="Nuxeo">
  <meta name="viewport" content="width=1024, user-scalable=no">

  <!-- Core and extension CSS files -->
  <link rel="stylesheet" href="${jaxrs.getResourceUrl("deck.core.css")}">
  <link rel="stylesheet" href="${jaxrs.getResourceUrl("deck.goto.css")}">
  <link rel="stylesheet" href="${jaxrs.getResourceUrl("deck.menu.css")}">
  <link rel="stylesheet" href="${jaxrs.getResourceUrl("deck.navigation.css")}">
  <link rel="stylesheet" href="${jaxrs.getResourceUrl("deck.status.css")}">
  <link rel="stylesheet" href="${jaxrs.getResourceUrl("deck.hash.css")}">
  <link rel="stylesheet" href="${jaxrs.getResourceUrl("deck.scale.css")}">

  <!-- Nuxeo Slide Default theme -->
  <link rel="stylesheet" href="${jaxrs.getResourceUrl("nuxeo-theme.css")}">

  <!-- Nuxeo Slide with comments theme -->
  <link rel="stylesheet" href="${jaxrs.getResourceUrl("nuxeo-theme-with-comments.css")}">

  <!-- Transition theme. More available in /themes/transition/ or create your own. -->
  <link rel="stylesheet" href="${jaxrs.getResourceUrl("nuxeo-transition.css")}">

  <!-- Nuxeo Slide Webinar theme -->
  <link rel="stylesheet" href="${jaxrs.getResourceUrl("nuxeo-webinar.css")}">

  <script src="${jaxrs.getResourceUrl("modernizr.custom.js")}"></script>


  <style>

/* -- 
Nuxeo Techonoly webinar - 2013
Based on the Default
=nuxeo world specific => specific rules for nuxeo world's need
-- */

/* =nuxeo world specific */

/* Background color */

.on-slide-nuxeo-slides, .on-slide-thank-you {

/* SVG fallback(Opera 11.10-, IE9) */
background: url(data:image/svg+xml;base64,);

/* Opera 11.10+ */
background: -o-linear-gradient(top, rgba(137,86,154,1), rgba(0,165,213,1));

/* Firefox 3.6+ */
background: -moz-linear-gradient(top, rgba(137,86,154,1), rgba(0,165,213,1));

/* Chrome 7+ & Safari 5.03+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0, rgba(137,86,154,1)), color-stop(1, rgba(0,165,213,1)));

/* IE5.5 - IE7 */
filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0,StartColorStr=#FF89569A,EndColorStr=#FF00A5D5);

/* IE8 */
-ms-filter: "progid:DXImageTransform.Microsoft.Gradient(GradientType=0,StartColorStr=#FF89569A,EndColorStr=#FF00A5D5)";

}

#nuxeo-slides, #thank-you {
  background:url("${jaxrs.getResourceUrl("outline-background.png")}") center center no-repeat;
}

.deck-container .nuxeo-logo .slash {
  background: url("${jaxrs.getResourceUrl("webinar-headset.png")}") top left no-repeat;
  width:32px;
  height:32px;
  display:inline-block;
  margin-bottom:-12px;
}

/* brickon each slide */

.deck-container .nuxeo-dev-logo {
  bottom: 0;
  color: #000;
  font-size: .5em;
  font-weight: bold;
  margin: 0;
  right: 5px;
  z-index: 3;
  position: fixed;
  background: url("${jaxrs.getResourceUrl("brick-alone.png")}") top left no-repeat;
  width:90px;
  height:53px;
}

body.on-slide-nuxeo-slides .nuxeo-dev-logo, body.on-slide-nuxeo-slides .deck-status, body.on-slide-nuxeo-slides .nuxeo-logo, body.on-slide-thank-you .nuxeo-dev-logo, body.on-slide-thank-you .deck-status, body.on-slide-thank-you .nuxeo-logo {
  display: none;
}

.deck-container .deck-status {
    bottom: 10px;
    font-size: 0.4em;
    right: 103px;
    color:rgba(100,100,100,0.8);
}

.slash span {display:none;}

/* Title */

#nuxeo-slides h1, #thank-you h2 {
  background-color: yellow;
  margin: 0 1.5em;
  padding: 0.3em 0;
  border:2px outset #fcde24;
  color:black;  
}

.csstransforms #nuxeo-slides h1, .csstransforms #nuxeo-slides .vcenter, 
.csstransforms #thank-you h2 {
  top:35%;
}

/* Authors */

.deck-container #nuxeo-slides h6, .deck-container #thank-you h6 {
  text-align:center;
  display:inline-block;
  background-color: rgba(255,255,255,0.4);
  padding:2em;
  position: absolute;
  top:55%;
  width:30%;
  left:50%;
  margin-left:-20%;
  font-size: 0.5em;
  }
</style>
</head>

<body class="deck-container">

<!-- Nuxeo Logo Training -->
<div class="nuxeo-logo">
  <span class="nuxeo">nuxeo</span>
  <span class="slash"><span>headset</span></span>
  <span class="sub-part training">Webinars</span>
</div>

<!-- Nuxeo World Logo -->
<div class="nuxeo-dev-logo">
</div>

<!-- Begin slides -->
<section class="slide cover" id="nuxeo-slides">
  <h1 id="slideDeckTitle">${doc.dublincore.title}</h1>
  <h6>  <div id="for-developers"><img src="${jaxrs.getResourceUrl("nuxeo-webinars-for-developers.png")}" alt="For Developers"/></div>
<#if doc.dublincore.rights?has_content>
  ${doc.dublincore.rights}
<#else>
  ${doc.dublincore.creator}
</#if>
<br/>&copy; 2013 - Nuxeo</h6>
<!-- paste this on the end slide too -->
</section>

${htmlContent}

<section class="slide" id="thank-you">
  <h2>Thank you!</h2>
 <h6>  <div id="for-developers"><img src="${jaxrs.getResourceUrl("nuxeo-webinars-for-developers.png")}" alt="For Developers"/></div>
${doc.dublincore.creator}<br/>&copy; 2013 - Nuxeo</h6>
</section>

<!-- deck.navigation snippet -->
<a href="#" class="deck-prev-link" title="Previous">&#8592;</a>
<a href="#" class="deck-next-link" title="Next">&#8594;</a>

<!-- deck.status snippet -->
<p class="deck-status">
	<span class="deck-status-current"></span>
	/
	<span class="deck-status-total"></span>
</p>

<!-- deck.goto snippet -->
<form action="." method="get" class="goto-form">
	<label for="goto-slide">Go to slide:</label>
	<input type="text" name="slidenum" id="goto-slide" list="goto-datalist">
	<datalist id="goto-datalist"></datalist>
	<input type="submit" value="Go">
</form>

<!-- deck.hash snippet -->
<a href="." title="Permalink to this slide" class="deck-permalink">#</a>


<!-- Grab CDN jQuery, with a protocol relative URL; fall back to local if offline -->
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="${jaxrs.getResourceUrl("jquery-1.7.2.min.js")}"><\/script>')</script>

<!-- Deck Core and extensions -->
<script src="${jaxrs.getResourceUrl("deck.core.js")}"></script>
<script src="${jaxrs.getResourceUrl("deck.hash.js")}"></script>
<script src="${jaxrs.getResourceUrl("deck.menu.js")}"></script>
<script src="${jaxrs.getResourceUrl("deck.goto.js")}"></script>
<script src="${jaxrs.getResourceUrl("deck.status.js")}"></script>
<script src="${jaxrs.getResourceUrl("deck.navigation.js")}"></script>
<script src="${jaxrs.getResourceUrl("deck.scale.js")}"></script>

<!-- integrate JS to change Markdown output-->
<script src="${jaxrs.getResourceUrl("mkslides.js")}"></script>

<!-- Initialize the deck -->
<script>
$(document).ready(function() {
 preProcessHtml("${jaxrs.getResourceUrl('')}");
 $.deck('.slide');
});
</script>

</body>
</html>]]></note:note>
  </schema>
</document>
