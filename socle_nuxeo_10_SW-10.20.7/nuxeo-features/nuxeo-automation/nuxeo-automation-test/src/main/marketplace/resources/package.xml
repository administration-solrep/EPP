<package type="addon" name="automation-test" version="@VERSION@">
  <title>Nuxeo Marketplace Automation Test Package</title>
  <description>
    <p>This addon is a Marketplace Automation Test Package for Testing purpose</p>
  </description>
  <home-page>http://doc.nuxeo.com/</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <hotreload-support>true</hotreload-support>
  <require-terms-and-conditions-acceptance>true</require-terms-and-conditions-acceptance>
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>testing</production-state>
  <supported>true</supported>
  <platforms>
    <platform>cap-@DISTRIB_VERSION@</platform>
  </platforms>
  <license>LGPL 2.1</license>
  <license-url>http://www.gnu.org/licenses/lgpl-2.1.html</license-url>
