<?xml version="1.0"?>
<component name="org.nuxeo.ecm.user.center.profile.UserProfileService.test.not-ok">

  <require>org.nuxeo.ecm.platform.ecm.user.center.profile.UserProfileService</require>

  <extension target="org.nuxeo.ecm.user.center.profile.UserProfileService"
    point="config">

    <importerConfig>
      <dataFile>data/docs_not_ok.csv</dataFile>
      <dateFormat>MM/dd/yyyy</dateFormat>
      <listSeparatorRegex>\|</listSeparatorRegex>
      <updateExisting>true</updateExisting>
      <batchSize>50</batchSize>
    </importerConfig>

  </extension>

</component>
