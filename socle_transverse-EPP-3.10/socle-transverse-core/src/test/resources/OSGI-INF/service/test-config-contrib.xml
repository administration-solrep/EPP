<?xml version="1.0"?>

<!-- Ce composant permet de contribuer des valeurs par dÃ©faut au service 
	de configuration de l'application, afin d'effectuer les tests unitaires. -->
<component name="fr.dila.st.core.test.service">
	<extension target="fr.dila.st.core.service.ConfigService"
		point="configParameter">
		<parameter>
			<name>app.some.string.value</name>
			<value>someName</value>
		</parameter>
		<parameter>
			<name>app.some.boolean.value</name>
			<value>true</value>
		</parameter>
		<parameter>
			<name>app.some.integer.value</name>
			<value>42</value>
		</parameter>
		<parameter>
			<name>app.some.double.value</name>
			<value>1.234</value>
		</parameter>
	</extension>
</component>