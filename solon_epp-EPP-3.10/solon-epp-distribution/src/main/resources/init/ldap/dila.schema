objectclass     ( 1.1 
                NAME 'gouvernement' 
                SUP groupOfUniqueNames 
                STRUCTURAL 
                MUST ( dateDebut $ label $ deleted )
                MAY ( dateFin $ lockDate $ lockUserName $ functionRead ) 
 ) 

objectclass     ( 1.2 
                NAME 'uniteStructurelle' 
                SUP groupOfUniqueNames 
                STRUCTURAL 
                MUST ( dateDebut $ type $ label $ deleted )
                MAY ( dateFin $ norDirectionMember $ lockDate $ lockUserName $ functionRead ) 
 )

objectclass     ( 1.2.1 
                NAME 'entite' 
                SUP groupOfUniqueNames 
                STRUCTURAL 
                MUST ( dateDebut $ label $ edition $ formule $ deleted )
                MAY ( dateFin $ norMinistere $ membreGouvernement $ membreGouvernementCivilite $ membreGouvernementPrenom $ membreGouvernementNom $ lockDate $ lockUserName $ ordre $ functionRead ) 
 )
 
objectclass     ( 1.3 
                NAME 'poste' 
                SUP groupOfUniqueNames 
                STRUCTURAL 
                MUST ( dateDebut $ label $ deleted )
                MAY ( dateFin $ uniqueMember $ superviseurSGG $ chargeMissionSGG $ conseillerPM $ posteBdc $ wsUrl $ wsUser $ wsPassword $ wsKeyAlias $ lockDate $ lockUserName $ functionRead $ posteWs ) 
 )

objectclass     ( 1.4
                NAME 'personne' 
                SUP inetOrgPerson 
                STRUCTURAL 
                MUST ( dateDebut $ temporary $ occasional $ deleted )
                MAY ( dateFin $ passwordReset )
 )

objectclass     ( 1.5 
                NAME 'institution' 
                SUP groupOfUniqueNames 
                STRUCTURAL 
                MUST ( label )
                MAY ( lockDate $ lockUserName )
 )

