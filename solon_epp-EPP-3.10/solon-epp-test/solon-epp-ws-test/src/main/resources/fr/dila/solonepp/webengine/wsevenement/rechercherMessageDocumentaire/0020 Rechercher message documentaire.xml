<?xml version="1.0" encoding="UTF-8"?>
<wsevt:rechercherEvenementRequest
	xmlns:wsevt="http://www.dila.premier-ministre.gouv.fr/solon/epp/WSevenement"
	xmlns:eppevt="http://www.dila.premier-ministre.gouv.fr/solon/epp/epp-evt"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.dila.premier-ministre.gouv.fr/solon/epp/WSevenement ../../../../../../../../xsd/solon/epp/WSevenement.xsd">

	<wsevt:par_requete>SELECT m.ecm:uuid as id FROM Message AS m WHERE
		m.dc:title = 'ID_EVENEMENT' AND testAcl(m.ecm:uuid) = 1</wsevt:par_requete>
</wsevt:rechercherEvenementRequest>
